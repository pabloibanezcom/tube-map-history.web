{
  "name": {
    "elementType": "input",
    "propertyType": "string",
    "elementConfig": {
      "type": "text",
      "placeholder": "Station name"
    },
    "validation": {
      "required": true
    }
  },
  "year": {
    "elementType": "input",
    "propertyType": "number",
    "elementConfig": {
      "type": "number",
      "placeholder": "Year"
    },
    "validation": {
      "required": false,
      "minLength": 4,
      "maxLength": 4
    }
  },
  "geometry": {
    "elementType": "place-search",
    "propertyType": "object",
    "elementConfig": {
      "label": "Location",
      "placeholder": "Type a station name..."
    }
  },
  "searchButton": {
    "elementType": "button",
    "elementConfig": {
      "type": "submit",
      "style": "secondary",
      "text": "Edit station"
    }
  }
}