{
  "name": {
    "elementType": "input",
    "propertyType": "string",
    "elementConfig": {
      "type": "text",
      "label": "Name",
      "floating": false,
      "placeholder": "Type a station name...",
      "clearable": true
    },
    "validation": {
      "required": true
    }
  },
  "year": {
    "elementType": "range",
    "propertyType": "object",
    "elementConfig": {
      "enabled": false,
      "toggleType": "primary",
      "label": "Year",
      "min": 1853,
      "max": 2019,
      "value": [
        1853,
        2019
      ]
    }
  },
  "numberLines": {
    "elementType": "range",
    "propertyType": "object",
    "elementConfig": {
      "enabled": false,
      "toggleType": "primary",
      "label": "Number of lines",
      "min": 0,
      "max": 8,
      "value": [
        1,
        8
      ]
    }
  },
  "line": {
    "elementType": "select",
    "propertyType": "object",
    "options": "lines",
    "elementConfig": {
      "label": "Line",
      "placeholder": "Select a line",
      "options": {
        "key": "_id",
        "label": "name",
        "value": "_id"
      },
      "noneLabel": "None",
      "custom": {
        "selected": "SelectedLine",
        "dropdown": "DropdownLine"
      },
      "enableSearch": "true"
    }
  },
  "searchButton": {
    "elementType": "button",
    "elementConfig": {
      "type": "submit",
      "style": "primary",
      "text": "Search"
    }
  }
}