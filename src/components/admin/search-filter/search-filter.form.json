{
  "name": {
    "elementType": "input",
    "propertyType": "string",
    "elementConfig": {
      "type": "text",
      "placeholder": "Station name"
    },
    "validation": {
      "required": true
    }
  },
  "year": {
    "elementType": "range",
    "propertyType": "object",
    "elementConfig": {
      "enabled": false,
      "toggleType": "secondary",
      "label": "Year",
      "min": 1853,
      "max": 2018,
      "value": [
        1853,
        2018
      ]
    }
  },
  "line": {
    "elementType": "select",
    "propertyType": "object",
    "elementConfig": {
      "label": "Line",
      "placeholder": "Select a line",
      "options": {
        "key": "_id",
        "label": "name",
        "value": "_id"
      },
      "noneLabel": "None",
      "custom": {
        "selected": "SelectedLine",
        "dropdown": "DropdownLine"
      },
      "enableSearch": "true",
      "remote": {
        "model": "line",
        "propertyName": "name",
        "minChars": 0
      }
    }
  },
  "searchButton": {
    "elementType": "button",
    "elementConfig": {
      "type": "submit",
      "text": "Search"
    }
  }
}